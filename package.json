{
  "name": "component-map",
  "version": "1.0.0",
  "description": "A storage for (not only) React components backed by WeakMap and performant fallback",
  "license": "MIT",
  "author": "Adam Stankiewicz",
  "main": "./lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/sheerun/component-map.git"
  },
  "bugs": {
    "url": "https://github.com/sheerun/component-map/issues"
  },
  "scripts": {
    "test": "node --expose-gc ./node_modules/.bin/ava --verbose --serial",
    "lint": "prettier --trailing-comma none --write '{src,test}/**/*.js'",
    "build": "rimraf lib && babel src --out-dir lib"
  },
  "files": [
    "lib"
  ],
  "keywords": [
    "react",
    "weak-map",
    "components",
    "memoize"
  ],
  "devDependencies": {
    "ava": "^0.15.2",
    "babel-core": "^6.7.7",
    "babel-preset-es2015": "^6.6.0",
    "core-js": "^2.4.1",
    "prettier": "^0.21.0",
    "react": "^15.1.0"
  },
  "ava": {
    "babel": "inherit",
    "require": [
      "babel-core/register"
    ]
  }
}
